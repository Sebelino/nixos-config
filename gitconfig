[alias]
    pr-checkout = "!f() { git fetch upstream pull/\"$1\"/head:PR-\"$1\"; git checkout PR-\"$1\"; }; f"
    rewritebranch = !"git rebase -i $(git merge-base master HEAD)"
    diffcommit = "!f(){ git diff HEAD~$(($1 + 1)) HEAD~$1; }; f"
    diffcommitn = "!f(){ git diff HEAD~$(($1 + 1)) HEAD~$1 --name-only; }; f"
[user]
	email = sebelino7@gmail.com
[rerere]
	enabled = true
