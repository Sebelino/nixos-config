{
  "title": "ISO < > key fix",
  "rules": [
    {
      "description": "Map grave_accent_and_tilde to non_us_backslash",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{ "key_code": "non_us_backslash" }]
        }
      ]
    }
  ]
}
