{
    "description": "Fix ISO < > key misdetected as grave_accent_and_tilde",
    "manipulators": [
        {
            "from": {
                "key_code": "grave_accent_and_tilde",
                "modifiers": { "optional": ["any"] }
            },
            "to": [{ "key_code": "non_us_backslash" }],
            "type": "basic"
        }
    ]
}
