{
  "title": "Solemak",
  "rules": [
    {
      "description": "Solemak for Mac",
      "manipulators": [
        {
          "type": "basic",
          "description": "Plus Modifier",
          "from": {
            "key_code": "hyphen",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "description": "Tab",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ]
        },
        {
          "type": "basic",
          "description": "RCommand + AKey",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "non_us_backslash"
            }
          ]
        },
        {
          "type": "basic",
          "description": "EKey",
          "from": {
            "key_code": "e",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "f"
            }
          ]
        },
        {
          "type": "basic",
          "description": "RKey",
          "from": {
            "key_code": "r",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "p"
            }
          ]
        },
        {
          "type": "basic",
          "description": "RCommand + RKey",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "non_us_backslash",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "description": "TKey",
          "from": {
            "key_code": "t",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "g"
            }
          ]
        },
        {
          "type": "basic",
          "description": "YKey",
          "from": {
            "key_code": "y",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "j"
            }
          ]
        },
        {
          "type": "basic",
          "description": "UKey",
          "from": {
            "key_code": "u",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "l"
            }
          ]
        },
        {
          "type": "basic",
          "description": "IKey",
          "from": {
            "key_code": "i",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "u"
            }
          ]
        },
        {
          "type": "basic",
          "description": "OKey",
          "from": {
            "key_code": "o",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "y"
            }
          ]
        },
        {
          "type": "basic",
          "description": "PKey",
          "from": {
            "key_code": "p",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "period"
            }
          ]
        },
        {
          "type": "basic",
          "description": "ÅKey",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ]
        },
        {
          "type": "basic",
          "description": "HatKey",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Capslock Modifier",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ]
        },
        {
          "type": "basic",
          "description": "SKey",
          "from": {
            "key_code": "s",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "r"
            }
          ]
        },
        {
          "type": "basic",
          "description": "RCommand + SKey",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "description": "DKey",
          "from": {
            "key_code": "d",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "s"
            }
          ]
        },
        {
          "type": "basic",
          "description": "FKey",
          "from": {
            "key_code": "f",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "t"
            }
          ]
        },
        {
          "type": "basic",
          "description": "GKey",
          "from": {
            "key_code": "g",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "d"
            }
          ]
        },
        {
          "type": "basic",
          "description": "JKey",
          "from": {
            "key_code": "j",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "n"
            }
          ]
        },
        {
          "type": "basic",
          "description": "KKey",
          "from": {
            "key_code": "k",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "e"
            }
          ]
        },
        {
          "type": "basic",
          "description": "LKey",
          "from": {
            "key_code": "l",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "i"
            }
          ]
        },
        {
          "type": "basic",
          "description": "ÖKey",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "o"
            }
          ]
        },
        {
          "type": "basic",
          "description": "ÄKey",
          "from": {
            "key_code": "quote",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Leftshift",
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab"
            }
          ]
        },
        {
          "type": "basic",
          "description": "NKey",
          "from": {
            "key_code": "n",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "k"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Comma",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "spacebar"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Dot",
          "description": "period -> ä",
          "from": {
            "key_code": "period",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "quote"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Minus",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "semicolon"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Space",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_shift"
            }
          ]
        }
      ]
    }
  ]
}
