          "from": {
            "key_code": "quote",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Left Shift -> Tab",
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "k"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Comma -> Space",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "spacebar"
            }
          ]
        },
        {
          "type": "basic",
          "description": "period -> ä",
          "from": {
            "key_code": "period",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "quote"
            }
          ]
        },
        {
          "type": "basic",
          "description": "slash -> ö",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "semicolon"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Space -> Right Shift",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_shift"
            }
          ]
        }
      ]
    }
  ]
