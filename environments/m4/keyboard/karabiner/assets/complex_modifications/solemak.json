{
  "title": "Solemak",
  "rules": [
    {
      "description": "Solemak (best-effort from XKB) + Caps=Ctrl + AE11=Hyper (Cmd+Ctrl+Opt+Shift)",
      "manipulators": [
        {
          "type": "basic",
          "description": "AE11 best-effort: hyphen key -> Hyper when held, '-' when tapped",
          "from": {
            "key_code": "hyphen",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "hyphen"
            }
          ]
        },
        {
          "type": "basic",
          "description": "AE11 fallback: equal_sign key -> Hyper when held, '=' when tapped",
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "equal_sign"
            }
          ]
        },
        {
          "type": "basic",
