              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "s"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "t"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
